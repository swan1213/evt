        "EventEmitter",
        "typescript",
        "functional programming",
        "qt signal",
        "events",
        "ts-evt",
        "async-await",
        "promise",
        "rxjs"
    ],
    "homepage": "https://evt.land",
    "devDependencies": {
        "@types/node": "^8.9.1",
        "rxjs": "^6.5.4",
        "typescript": "^3.8.0",
        "denoify": "github:garronej/denoify"
    }
